@namespace ExtraDry.Blazor
@implements IDisposable

@if (ResolvedCommand?.IsVisible() ?? false)
{
    <Button CssClass="@CssClasses" Caption="@(ShowCaption?ResolvedCommand.Caption:string.Empty)" @attributes="@UnmatchedAttributes"
        Icon="@ResolvedCommand.Icon" Affordance="@ResolvedCommand.Affordance" disabled="@disabled"
        @onclick="DoClick" />
}
