@typeparam TModel
@inherits DryFieldBase<TModel>

<OptionField TValue="Option" CssClass="@CssClasses"
    Affordance="@ResolvedAffordance"
    Description="@ResolvedDescription"
    Icon="@ResolvedIcon" 
    Label="@ResolvedLabel" 
    Placeholder="@ResolvedPlaceholder" 
    ReadOnly="@ResolvedReadOnly"
    @bind-Value="Value" 
    Options="@Options"
    Validator="Validator"
    OnChange="HandleChange" 
    OnInput="HandleInput"
    OnValidate="HandleValidate"
    />
